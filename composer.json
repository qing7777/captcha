{
    "name": "qing7777/captcha",
    "type": "package",
    "description": "Hyperf Captcha Package",
    "keywords": [
        "Hyperf Captcha",
        "Captcha"
    ],
    "homepage": "https://github.com/qing7777/captcha",
    "license": "MIT",
    "authors": [
        {
            "name": "qing7260",
            "email": "qing7260@outlook.com",
            "homepage": "https://github.com/qing7260",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-gd": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Qing7777\\Captcha\\": "src/"
        },
        "files": [
            "src/helpers.php",
            "src/ConfigProvider.php"
        ]
    },
    "extra": {
        "hyperf": {
            "config": "Qing7777\\Captcha\\ConfigProvider"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ],
        "psr-4": {
            "Qing7777\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
